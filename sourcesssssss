	local function FDOHQ_fake_script() -- TextButton.LocalScript 
		local script = Instance.new('LocalScript', TextButton)

		script.Parent.MouseButton1Click:Connect(function()
			local Message ="A_1"
			local A_1Text ="[Panle]: File type allintext: [Log,2]. Please Check the Console!"
			local A_2 ="All"

			while true do
				wait(1)
				local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
				wait(0.5)
				A_1Text:Destroy()
				wait(0)
				A_2:Destroy()
			end

			print("[Panle]: AntiLogs")
		end)
	end
	coroutine.wrap(FDOHQ_fake_script)()
