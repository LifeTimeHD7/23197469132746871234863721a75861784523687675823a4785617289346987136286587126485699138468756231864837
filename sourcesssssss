	local function HZAVPS_fake_script() -- main_bar.Mover 
		local script = Instance.new('LocalScript', main_bar)

		while true do
			wait(2)
			script.Parent.Size = UDim2.new(0, 28,0, 2)
			wait(2)
			script.Parent.Size = UDim2.new(0, 37,0, 2)
			wait(4)
			script.Parent.Size = UDim2.new(0, 93,0, 2)
			wait(2)
			script.Parent.Size = UDim2.new(0, 100,0, 2)
			wait(1)
			script.Parent.Size = UDim2.new(0, 148,0, 2)
			wait(3)
			script.Parent.Size = UDim2.new(0, 207,0, 2)
			wait(2)
			script.Parent.Size = UDim2.new(0, 212,0, 2)
			wait(4)
			script.Parent.Size = UDim2.new(0, 221,0, 2)
			wait(1)
			script.Parent.Size = UDim2.new(0, 252,0, 2)
			wait(2)
			script.Parent.Parent.Parent.Parent.Main.Visible = true
			wait(1)
			script.Parent.Parent.Parent:Destroy()
		end
	end
	coroutine.wrap(HZAVPS_fake_script)()
