							return Tool
						end
					end
				end

				local function GetGripForHandle(Handle)
					for _, Weld in next, Character:GetDescendants() do
						if Weld:IsA("Weld") and (Weld.Part0 == Handle or Weld.Part1 == Handle) then
							return Weld
						end
