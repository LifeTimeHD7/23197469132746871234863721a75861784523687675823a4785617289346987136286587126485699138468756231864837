	local function FKSKY_fake_script() -- Main.LocalScript 
		local script = Instance.new('LocalScript', Main)

		wait(25) --[[ Waits 25 untill Runs Scripts ]]--

		local newsound = Instance.new("Sound") --[[ Creates new Sound ]]--
		s = newsound --[[ Makes s to newsound ]]--

		s.Parent = game.SoundService --[[ Replaces s/newsound to the SoundService in the game! ]]--
		s.SoundId ="rbxassetid://1864363649" --[[ Gives s/newsound a SoundId ]]--
		s.Playing = true --[[ Plays Music ]]--
		s.Looped = true --[[ Loops s/newsound ]]--
		s.Archivable = true --[[ Makes s/newsound Archvable ]]--
		s.Name ="INSTANCEBYPANLEHUB" --[[ Gives s/newsound a Name ]]--
		s.PlaybackSpeed = 1 --[[ Gives Speed to s/newsound ]]--
		s.Volume = 0.5 --[[ Gives s/newsound a Volume of 0.5/Starter Volume ]]--

		--[[ DONT DELET THIS THIS IS THE REPLACER HELPER! ]]--

		if s.Parent == workspace then --[[ if s/newsound = to workspace Wich could happen than it will take it from Workspace and put it back into SoundService ]]--
			s.Parent = game.SoundService --[[ Putts s/newsound back to SoundService if it Gliches to Workspace! ]]--
		end --[[ Ends ]]--

		local ReverbSoundEffect = Instance.new("ReverbSoundEffect") --[[ Creates ReverbSoundEffect ]]--
		r = ReverbSoundEffect --[[ Makes r to ReverbSoundEffect ]]--

		r.Parent = game.SoundService.INSTANCEBYPANLEHUB --[[ Replaces r/ReverbSoundEffect into s/newsound in SoundService in game! ]]--
		r.Name ="HALLEFFECTBYPANLE"

		--[[ DONT DELET THIS THIS IS THE REPLACER HELPER! ]]--

		if r.Parent == workspace then --[[ if r/ReverbSoundEffect = to Workspace Wich Could Happen Than it will take it from Workspace and put it back into s/newsound ]]--
			r.Parent = game.SoundService.INSTANCEBYPANLEHUB --[[ Replaces r/ReverbSoundEffect into s/newsound ]]--
		end --[[ Ends ]]--
	end
	coroutine.wrap(FKSKY_fake_script)()
	local function CBYZQNF_fake_script() -- Main.LocalScript 
		local script = Instance.new('LocalScript', Main)

		local UIS = game:GetService('UserInputService')
		local frame = script.Parent
		local dragToggle = nil
		local dragSpeed = 0.25
		local dragStart = nil
		local startPos = nil

		local function updateInput(input)
			local delta = input.Position - dragStart
			local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
				startPos.Y.Scale, startPos.Y.Offset + delta.Y)
			game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
		end

		frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
				dragToggle = true
				dragStart = input.Position
				startPos = frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)

		UIS.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				if dragToggle then
					updateInput(input)
				end
			end
		end)
	end
	coroutine.wrap(CBYZQNF_fake_script)()
